pipeline {
    agent any

    stages {
        stage('Clonar repositório') {
            steps {
               git branch: 'main', url: 'https://github.com/Joseane-Guedes/testes-api-cy.git' 
            }
        }
                stage('Instalar dependências') {
            steps {
                sh 'npm install -f'
            }
        }
                stage('Executar testes') {
            steps {
                 sh 'npm run cy:run'
            }
        }
    }
}